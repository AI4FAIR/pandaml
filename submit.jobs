export LUSTRE_HOME="/lustre/panda/"$USER
export SLURM_SUBMIT_DIR=$LUSTRE_HOME"/pandaml"
export SLURM_WORKING_DIR=$LUSTRE_HOME"/pandaml/logs"
mkdir -p $SLURM_WORKING_DIR;
